      "parameters": [
        {
          "name": "structName",
          "type": "String",
          "description": "结构体名称",
          "direction": "in"
        },
        {
          "name": "structPtr",
          "type": "Long",
          "description": "结构体指针",
          "direction": "in"
        }
      ],
      "returnType": "Variant",
      "example": "result = ZINDEX_STRUCT(\"MyStruct\", ptr).memberName"
    }
  ]
}